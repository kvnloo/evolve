{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://github.com/YOUR_USERNAME"
    },
    {
      "pattern": "^https://github.com/\\{org\\}/\\{repo\\}"
    },
    {
      "pattern": "^https://claude\\.ai/claude-code"
    },
    {
      "pattern": "^https://github.com/superclaude/framework"
    },
    {
      "pattern": "^link$"
    },
    {
      "pattern": "\\$ARGUMENTS"
    },
    {
      "pattern": "^research/.*\\.md$"
    },
    {
      "pattern": "^docs/.*\\.md$"
    },
    {
      "pattern": "^CLAUDE\\.md$"
    },
    {
      "pattern": "^\\.\\./memory/memory-usage\\.md$"
    },
    {
      "pattern": "^\\./agent-migration"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "Accept-Encoding": "zstd, br, gzip, deflate"
      }
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
